{
  "brackets": [
    ["{", "}"],
    ["[", "]"]
  ],
  "autoClosingPairs": [
    {
      "open": "{",
      "close": "}"
    },
    {
      "open": "[",
      "close": "]"
    },
    {
      "open": "\"",
      "close": "\"",
      "notIn": ["string"]
    }
  ],
  "colorizedBracketPairs": [
		["{", "}"]
	],
  "autoCloseBefore": ";:.,=}] \n\t\"",
